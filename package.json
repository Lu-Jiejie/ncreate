{
  "name": "ncreate",
  "type": "module",
  "version": "0.0.6",
  "packageManager": "pnpm@10.15.0",
  "description": "Create your new project from a NPM, GitHub or local template with a single command.",
  "author": "Lu-Jiejie",
  "license": "MIT",
  "homepage": "https://github.com/Lu-Jiejie/ncreate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lu-Jiejie/ncreate.git"
  },
  "bugs": "https://github.com/Lu-Jiejie/ncreate/issues",
  "keywords": [
    "cli",
    "create",
    "NPM"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "ncreate": "./bin/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "start": "tsx src/index.ts",
    "cli": "tsx src/runner.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "nr build",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.18.0",
    "bumpp": "^10.2.3",
    "eslint": "^9.34.0",
    "execa": "^9.6.0",
    "gitignore-to-minimatch": "^0.0.2",
    "https-proxy-agent": "^7.0.6",
    "lint-staged": "^16.1.5",
    "minimatch": "^10.0.3",
    "minimist": "^1.2.8",
    "picocolors": "^1.1.1",
    "prompts-plus": "^3.0.4",
    "simple-git-hooks": "^2.13.1",
    "tar": "^7.4.3",
    "tsdown": "^0.14.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
