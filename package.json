{
  "name": "ncreate",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@9.1.1",
  "description": "Create your new project from a NPM, GitHub or local template with a single command.",
  "author": "Lu-Jiejie",
  "license": "MIT",
  "homepage": "https://github.com/Lu-Jiejie/ncreate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lu-Jiejie/ncreate.git"
  },
  "bugs": "https://github.com/Lu-Jiejie/ncreate/issues",
  "keywords": [
    "cli",
    "create",
    "NPM"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "ncreate": "./bin/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "cli": "esno src/runner.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "nr build",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "execa": "^9.0.2",
    "gitignore-to-minimatch": "^0.0.1",
    "https-proxy-agent": "^7.0.4",
    "minimatch": "^9.0.4",
    "minimist": "^1.2.8",
    "picocolors": "^1.0.0",
    "prompts-plus": "^3.0.4",
    "tar": "^7.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.17.0",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.12.11",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
